/************************************************ QDateTimeEdit  QTimeEdit QDateEdit *******************************************************/

QDateTimeEdit,
QDateEdit
{ 
	font: 12pt "Arial";
/*	width: 120px;*/
	height: 35px;
	background-color: transparent;
	border: 1px solid rgb(115,185,255);
	padding:0px;
	border-radius:15px;
}

QTimeEdit
{ 
	font: 12pt "Arial";
/*	width: 120px;*/
	height: 35px;
	background-color: transparent;
	border: 1px solid rgb(115,185,255);
	padding:0px;
	border-radius:15px;
}

QDateEdit::down-button,
QTimeEdit::down-button,
QDateTimeEdit::down-button,
QDateEdit::up-button,
QTimeEdit::up-button,
QDateTimeEdit::up-button
{
	subcontrol-origin:padding;
	background-color: transparent;
	border:none;
	min-width:26px;
	min-height:36px;
        border-radius:5px;
}

QDateEdit::down-button,
QTimeEdit::down-button,
QDateTimeEdit::down-button
{
	subcontrol-position:left center;
}

QDateEdit::up-button,
QTimeEdit::up-button,
QDateTimeEdit::up-button
{
	subcontrol-position:right center;
}

QDateEdit::down-button:focus,
QTimeEdit::down-button:focus,
QDateTimeEdit::down-button:focus,
QDateEdit::up-button:focus,
QTimeEdit::up-button:focus,
QDateTimeEdit::up-button:focus
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	                                 stop: 0 whitesmoke,
	                                 stop: 0.7 lightgray,
	                                 stop: 1 whitesmoke);
	margin: 0px;
	border: 1px solid transparent;
	border-radius: 5px;
}

QDateEdit::down-button:disabled:focus,
QTimeEdit::down-button:disabled:focus,
QDateTimeEdit::down-button:disabled:focus,
QDateEdit::up-button:disabled:focus,
QTimeEdit::up-button:disabled:focus,
QDateTimeEdit::up-button:disabled:focus
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	                                 stop: 0 gainsboro,
	                                 stop: 0.7 lightgray,
	                                 stop: 1 gainsboro);
}

QDateEdit::down-button:pressed,
QTimeEdit::down-button:pressed,
QDateTimeEdit::down-button:pressed,
QDateEdit::up-button:pressed,
QTimeEdit::up-button:pressed,
QDateTimeEdit::up-button:pressed
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 lavender, stop: 1 whitesmoke);
	margin: 0px;
	position: relative;
	top: 1px;
	left: 1px;
}

QDateEdit::down-arrow,
QTimeEdit::down-arrow,
QDateTimeEdit::down-arrow
{
	image:url(:/CMainWin/Resources/images/public/Calendar_left.png);
}

QDateEdit::up-arrow,
QTimeEdit::up-arrow,
QDateTimeEdit::up-arrow
{
	image:url(:/CMainWin/Resources/images/public/Calendar_right.png);
}


