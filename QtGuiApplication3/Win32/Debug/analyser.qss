QComboBox
{
	border: 1px solid rgb(115,185,255);
	border-radius: 5px;
	padding-left: 5px;
	min-height: 33px;
	min-width: 60px;
	font: 12pt "Microsoft YaHei";
}

QComboBox:disabled
{
    background-color:lightgray;
    border: lightgray;
}

QComboBox::item
{
    font: 12pt "Microsoft YaHei";
}

QComboBox:!editable
{
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                       stop: 0 #f6f7fa, 
				       stop: 0.7 lightgray,
				       stop: 1 #dadbde);
}


QComboBox:on
{
    padding-top: 3px;
    padding-left: 4px;
}


QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    min-width: 27px;
}


QComboBox QAbstractItemView
{
    outline: 0px;
    border: 1px solid rgb(115,185,255);
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    selection-background-color: rgb(62,195,255);
    font: 12pt "Microsoft YaHei";
}


QComboBox QAbstractItemView::item
{
    height: 32px;
    min-width: 30px;
}

QComboBox QAbstractItemView::item:selected
{
    background-color: rgb(62,195,255);
}

QComboBox#comboxQCAssay, #comboxQCConc
{
    color: rgb(0,147,211);
}

/***************************************************QTableWidget**********************************************************/
QTableWidget
{
    background-color: #D6ECF0;
    color: #0;
    border: 3px ridge rgb(0,147,211);
    show-decoration-selected: 1;
    alternate-background-color: rgb(176, 215, 255);
    selection-background-color: rgb(0,147,211);
    font: 12pt "Microsoft YaHei";
}


QTableWidget QHeaderView::section
{
    min-height: 40px;
    color: white;
    border-right: 2px solid white;
    border-top-right-radius: 0px;
    border-top-left-radius: 0px;
    background-color: rgb(0,147,211);
    font: 12pt "Microsoft YaHei";
}

QTableWidget QHeaderView::section:enabled
{
    color: white;
}

QTableWidget QHeaderView::section:disabled
{
    color: rgb(153,153,153);
}



QTableWidget::item
{
    border-right: 1px solid white;
    border-top: 1px solid rgb(62,195,255);
    padding: -1px;
    min-width: 106.75px;
    min-height: 50px;
}


/*
QTableWidget::item:hover
{
    background-color: rgb(0,147,211);
}*/


QTableWidget::item:selected
{
    background-color: rgb(0,147,211);
}



QTableWidget#tableWidgetFault QHeaderView::section
{
    color: black;
    border-right: 1px solid rgb(211,232,255) ;
    background-color: white;
}

QTableWidget#tableWidgetFault
{
    background-color: rgb(211,232,255);
}


QTableWidget#tableWidget_Sample QHeaderView::section
{
    min-height: 47px;
}

QTableWidget#tableWidget_QcOrder QHeaderView::section
{
    min-height: 45px;
    max-height: 45px;
}

QTableWidget#tableWidget_Qc QHeaderView::section
{
    min-height: 45px;
    max-height: 45px;
}



/***************************************************QScrollBar**********************************************************/
QScrollBar:vertical
{
    width: 30px;
    background-color: #D6ECF0;
    margin: 0px,0px,0px,0px; 
    padding-top: 35px; 
    padding-bottom: 35px; 
}

QScrollBar::handle:vertical
{
    background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
				     stop: 0 whitesmoke,
				     stop: 0.7 rgb(0,147,211),
				     stop: 1 whitesmoke); 
    border-radius: 5px;
}

/*
QScrollBar::handle:vertical:hover
{
    background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
				     stop: 0 whitesmoke,
				     stop: 0.7 rgb(62,195,255),
				     stop: 1 whitesmoke); 
}*/

QScrollBar::add-line:vertical
{
    border: 1px solid white;
}

QScrollBar::sub-line:vertical
{
    border: 1px solid white;
}

QScrollBar::add-page:vertical 
{
    background-color: #D6ECF0; 
}


QScrollBar::sub-page:vertical 
{
    background-color: #D6ECF0; 
}


QScrollBar::up-arrow:vertical 
{
    image: url(":/CMainWin/Resources/images/public/sort_up.png");
}

QScrollBar::down-arrow:vertical
{
    image: url(":/CMainWin/Resources/images/public/sort_down.png");
}


QScrollBar::up-arrow:vertical:pressed,
QScrollBar::down-arrow:vertical:pressed
{
    position: relative;
    top: 2px;
    left: 2px;
}



QScrollBar:horizontal
{
    height: 30px;
    background-color: #D6ECF0;
    padding-left: 35px;
    padding-right: 35px;
}


QScrollBar::handle:horizontal
{
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
				     stop: 0 whitesmoke,
				     stop: 0.7 rgb(0,147,211),
				     stop: 1 whitesmoke); 
    border-radius: 5px;
}

/*
QScrollBar::handle:horizontal:hover
{
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
			     stop: 0 whitesmoke,
			     stop: 0.7 rgb(62,195,255),
			     stop: 1 whitesmoke); 
}*/


QScrollBar::add-line:horizontal
{
    border: 1px solid white;
}

QScrollBar::sub-line:horizontal
{
    border: 1px solid white;
}

QScrollBar::add-page:horizontal 
{
    background-color: #D6ECF0; 
}


QScrollBar::sub-page:horizontal 
{
    background-color: #D6ECF0; 
}


QScrollBar::left-arrow:horizontal 
{
    image: url(":/CMainWin/Resources/images/public/Left_arrow.png");
}


QScrollBar::right-arrow:horizontal
{
    image: url(":/CMainWin/Resources/images/public/Right_arrow.png");
}


QScrollBar::left-arrow:horizontal:pressed,
QScrollBar::right-arrow:horizontal:pressed
{
    position: relative;
    top: 2px;
    right: 2px;
}


/***************************************************QTabWidget**********************************************************/
QTabWidget
{
    background-color: transparent;
}


QTabWidget::pane
{
    border: 0px solid rgb(176, 215, 255);
}


QTabWidget::tab-bar
{
    left: 0px;
}


QTabWidget#tabWidget_Maintain::pane 
{
    background-color: rgb(176,  215, 255);
}

QTabWidget#tabWidget_Result::pane 
{
    background-color: #D6ECF0;
}



/***************************************************QTabBar*************************************************************/

QTabBar:tab
{
    color: white;
    min-height: 40px;
    min-width: 111.25px;
    margin-right: 1px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px; 
    font: 12pt "Microsoft YaHei";
}



QTabBar::tab:selected
{
    background-color: rgb(0, 89, 157);
}
 
QTabBar::tab:!selected
{
    background-color: rgb(0, 147, 221);
}
 
