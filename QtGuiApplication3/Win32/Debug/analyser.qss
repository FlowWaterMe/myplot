/***************************************************QCheckBox**********************************************************/
QCheckBox
{
    spacing: 5px;
    font: 12pt "Microsoft YaHei";
    min-width: 30px;
    min-height: 30px;
    color: black;
    outline: none;
}

QCheckBox::indicator
{
    height: 25px;
    width: 25px;
    subcontrol-origin: padding;
    subcontrol-position: left center;
    background-color: transparent;
    margin: 0px;
    border: 1px solid transparent;
}


QCheckBox::indicator:unchecked
{
    image: url(":/CMainWin/Resources/images/public/cbx_uncheck.png");
}

QCheckBox::indicator:checked
{
    image: url(":/CMainWin/Resources/images/public/cbx_check.png");
}



