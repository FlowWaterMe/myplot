/************************************************ QSlider **********************************************************/
QPageSlider
{
    font:12pt "Microsoft YaHei";
    height: 20px; 
    color:rgb(127,127,127);
}

QPageSlider::groove:horizontal {
    height: 20px; 
    background-image: url(":/CMainWin/Resources/images/public/slider_line.png");
    background-position:center;
    background-repeat: repeat-x;
    margin-right:48px;
    margin-left: 10px;
}

QPageSlider::handle:horizontal 
{
    background-image: url(":/CMainWin/Resources/images/public/slider_hand.png");
    background-repeat:no-repeat;background-position: center;
    width: 25px;
    margin: -9px;            
}

